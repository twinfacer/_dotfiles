#!/usr/bin/env zsh

check_cmd() {
  if [[ $(which $1) =~ 'not\sfound' ]]
  then
    print -P "$1 is %F{green}free%f"
  else
    print -P "$1 is %F{red}used%f"
  fi
}

build_variants() {

}

for first_letter in {a..z}; do
  for second_letter in {a..z}; do
    check_cmd "$first_letter$second_letter"
  done
done
